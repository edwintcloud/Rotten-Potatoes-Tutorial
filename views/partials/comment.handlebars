<div class="card" id= "{{this._id}}">
    <div class="card-block">
        <h4 class="card-title">{{this.title}}</h4>
        <p class="card-text">{{this.content}}</p>
        <!-- Delete link -->
        <p>
                <button class="btn btn-link" type="button" id="deleteComment-{{slice8 this._id}}" onclick="commentDelete('{{slice8 this._id}}');" comment-id={{this._id}} movie-id={{this.movieId}}>Delete</button>
        </p>
    </div>
</div>
